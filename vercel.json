{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/token",
      "dest": "/api/token.js"
    },
    {
      "src": "/api/tokens", 
      "dest": "/api/tokens.js"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    }
  ]
}