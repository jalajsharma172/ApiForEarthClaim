{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/token",
      "dest": "/api/token.js"
    },
    {
      "src": "/api/tokens", 
      "dest": "/api/tokens.js"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    }
  ]
}